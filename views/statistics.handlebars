<script>
  $.ajax({
    url: "/api/ydata",
    type: "GET",
  }).then(function (data) {
    var data = [
      {
        x: [
          "Planning to apply",
          "Applied",
          "Phone Screen",
          "On Site Interview",
          "Offers",
          "Rejected",
        ],
        y: data,
        type: "bar",
      },
    ];

    Plotly.newPlot("graphDiv", data);
  });
</script>
